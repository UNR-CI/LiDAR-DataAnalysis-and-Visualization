{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "LiDarWebApp": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/LiDarWebApp",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/draco_decoder.wasm",
              {
                "glob": "**/*",
                "input": "node_modules/cesium/Build/Cesium/Workers",
                "output": "/assets/cesium/Workers"
              },
              {
                "glob": "**/*",
                "input": "node_modules/cesium/Build/Cesium/ThirdParty",
                "output": "/assets/cesium/ThirdParty"
              },
              {
                "glob": "**/*",
                "input": "node_modules/cesium/Build/Cesium/Assets",
                "output": "/assets/cesium/Assets"
              },
              {
                "glob": "**/*",
                "input": "node_modules/cesium/Build/Cesium/Widgets",
                "output": "/assets/cesium/Widgets"
              }
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css",
              "node_modules/cesium/Build/Cesium/Widgets/widgets.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }, 
            "quick":{
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "LiDarWebApp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "LiDarWebApp:build:production"
            },
            "quick": {
              "browserTarget": "LiDarWebApp:build:quick"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "LiDarWebApp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/cesium/Build/Cesium/Workers",
                "output": "/assets/cesium/Workers"
              },
              {
                "glob": "**/*",
                "input": "node_modules/cesium/Build/Cesium/ThirdParty",
                "output": "/assets/cesium/ThirdParty"
              },
              {
                "glob": "**/*",
                "input": "node_modules/cesium/Build/Cesium/Assets",
                "output": "/assets/cesium/Assets"
              },
              {
                "glob": "**/*",
                "input": "node_modules/cesium/Build/Cesium/Widgets",
                "output": "/assets/cesium/Widgets"
              }
            ],
            "styles": [
              "node_modules/cesium/Build/Cesium/Widgets/widgets.css",
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "LiDarWebApp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "LiDarWebApp:serve:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "b75e78d4-3831-4dc4-b514-d3d6c99dd61b"
  }
}
